{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://ezkar.in",
      "permanent": true
    },
    {
      "source": "https://www.ezkar.in/:path*",
      "destination": "https://ezkar.in/:path*",
      "permanent": true
    },
    {
      "source": "https://www.:tenant.ezkar.in/:path*",
      "destination": "https://:tenant.ezkar.in/:path*",
      "permanent": true
    }
  ]
}

{
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        { "type": "host", "value": "www.ezkar.in" }
      ],
      "destination": "https://ezkar.in/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        { "type": "host", "value": "www.:tenant.ezkar.in" }
      ],
      "destination": "https://:tenant.ezkar.in/:path*",
      "permanent": true
    }
  ]
}
